<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="rectangle" android:padding="10dp">
            <gradient
                android:angle="45"
                android:endColor="#60FDFDFD"
                android:startColor="#60FDFDFD"
                android:type="linear" />
            <padding
                android:bottom="0dp"
                android:left="0dp"
                android:right="0dp"
                android:top="0dp" />
            <stroke android:width="1dp" android:color="#7A6E6E"></stroke>
        </shape>
    </item>

</layer-list>